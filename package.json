{
    "name": "node-red-hmpx-example",
    "description": "Example flow for reading Vaisala Indigo HMPx humidity and temperature probes",
    "version": "1.0.0",
    "dependencies": {
        "node-red": "^0.20.3",
        "node-red-contrib-modbus": "4.1.3",
        "node-red-dashboard": "2.14.0"
    },
	"repository": {
  "type": "git",
  "url": "https://github.com/RuonalaJ/node-red-hmpx-example"
	},
	  "author": "Jarkko Ruonala",
    "license": "MIT",
    "node-red": {
        "settings": {
            "flowFile": "flow.json",
            "credentialsFile": "flow_cred.json"
        }
    },
    "scripts": {
        "start": "node ./node_modules/node-red/red.js --userDir . --settings ./node_modules/node-red/settings.js flow.json"
    }
}